@Article{Beilinson2010,
  author        = {Alexander Beilinson},
  title         = {Remarks on Grothendieck's standard conjectures},
  __markedentry = {[cedric:6]},
  abstract      = {We show that Grothendieck's standard conjectures are implied by either of two other motivic conjectures: (a) by that of the existence of the motivic t-structure, and (b) by (a weak form of) Suslin's Lawson homology conjecture.},
  date          = {2010-06-06},
  eprint        = {1006.1116v2},
  eprintclass   = {math.AG},
  eprinttype    = {arXiv},
  file          = {online:http\://arxiv.org/pdf/1006.1116v2:PDF},
  keywords      = {math.AG},
}

@Article{Ellis2015,
  author        = {Alexander P. Ellis and Ina Petkova and Vera VÃ©rtesi},
  title         = {Quantum gl(1|1) and tangle Floer homology},
  __markedentry = {[cedric:6]},
  abstract      = {We identify the Grothendieck group of the tangle Floer dg algebra with a tensor product of certain $U_q(gl(1|1))$ representations. Under this identification, up to a scalar factor, the map on the Grothendieck group induced by the tangle Floer dg bimodule associated to a tangle agrees with the Reshetikhin-Turaev homomorphism for that tangle. We also introduce dg bimodules which act on the Grothendieck group as the generators $E$ and $F$ of $U_q(gl(1|1))$.},
  date          = {2015-10-12},
  eprint        = {1510.03483v1},
  eprintclass   = {math.GT},
  eprinttype    = {arXiv},
  file          = {online:http\://arxiv.org/pdf/1510.03483v1:PDF},
  keywords      = {math.GT, math.QA, 57M27, 20G42},
}

@Article{Lubotzky2017,
  author        = {Alexander Lubotzky and T. N. Venkataramana},
  title         = {The congruence topology, Grothendieck duality and thin groups},
  __markedentry = {[cedric:6]},
  abstract      = {This paper answers a question raised by Grothendieck in 1970 on the "Grothendieck closure" of an integral linear group and proves a conjecture of the first author made in 1980. This is done by a detailed study of the congruence topology of arithmetic groups, obtaining along the way, an arithmetic analogue of a classical result of Chevalley for complex algebraic groups. As an application we also deduce a group theoretic characterization of thin subgroups of arithmetic groups.},
  date          = {2017-09-18},
  eprint        = {1709.06179v1},
  eprintclass   = {math.GR},
  eprinttype    = {arXiv},
  file          = {online:http\://arxiv.org/pdf/1709.06179v1:PDF},
  keywords      = {math.GR},
}

